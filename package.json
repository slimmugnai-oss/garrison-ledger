    "type-check": "tsc --noEmit",
    "validate": "npm run type-check && npm run lint && npm run secret-scan",
    "pre-commit": "npm run type-check && npm run lint",
    "pre-deploy": "npm run health-check && npm run check-icons && npm run secret-scan && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
